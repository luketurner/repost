{
  "name": "repost",
  "version": "0.1.0",
  "description": "Low-friction, automation-friendly HTTP testing in the terminal",
  "main": "index.js",
  "bin": "bin/repost.js",
  "scripts": {
    "cli": "node bin/repost.js",
    "debug": "node --inspect-brk bin/repost.js",
    "test": "jest --config=test/jest.config.js",
    "test:watch": "npm run test -- --watch",
    "docs": "rm -rf ./docs && jsdoc -c jsdoc.json",
    "docs:serve": "npm run docs && http-server ./docs -a localhost",
    "docs:watch": "nodemon --watch src --watch -e js,md tutorials --exec \"npm run docs:serve\""
  },
  "files": [
    "src",
    "bin"
  ],
  "author": "Luke Turner",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.0",
    "cli-of-mine": "^2.0.2",
    "command-line-args": "^5.1.1",
    "command-line-usage": "^6.0.2",
    "ejs": "^2.6.1",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.17.0",
    "express": "^4.16.3",
    "http-server": "^0.11.1",
    "jest": "^24.9.0",
    "jsdoc": "^3.6.3",
    "mock-fs": "^4.10.1",
    "nodemon": "^2.0.0"
  }
}
